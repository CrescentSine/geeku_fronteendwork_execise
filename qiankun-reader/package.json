{
  "name": "qiankun-reader",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "install": "npm-run-all --serial install:*",
    "install:main": "cd main && npm i",
    "install:sidebar": "cd sidebar && npm i",
    "install:content": "cd content && npm i",
    "dev": "npm-run-all --parallel dev:*",
    "dev:main": "cd main && npm run dev",
    "dev:sidebar": "cd sidebar && npm run dev",
    "dev:content": "cd content && npm run dev",
    "start": "npm-run-all --parallel start:*",
    "start:main": "cd main && npm start",
    "start:sidebar": "cd sidebar && npm start",
    "start:content": "cd content && npm start",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "CrescentMoon",
  "license": "CC0-1.0",
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
